{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [".", "src"],
    "includePattern": ".+\\.js(x|doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs"
  }
}
